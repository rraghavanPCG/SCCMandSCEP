        <AddValue Name="DisableRealtimeMonitoring" Type="REG_DWORD">1</AddValue>
        <AddValue Name="DisableOnAccessProtection" Type="REG_DWORD">1</AddValue>
        <AddValue Name="RealTimeScanDirection" Type="REG_DWORD">0</AddValue>
